{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "mdx": {
    "checkMdx": false,
  },
  "ts-node": {
    "typeCheck": true,
  },
  "compilerOptions": {
    "checkJs": true
  },
  "buildOptions": {
    "incremental": false
  },
  "typeAcquisition": {
    "enable": false
  },
  "compileOnSave": false  
}
